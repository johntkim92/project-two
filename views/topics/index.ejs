<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>All Topics</title>
  </head>
  <body>
    <% topics.forEach(function (topic, i) { %>
      <div id="topic-index">
        <h3>
          <a id="indiv-topics" href ="/topics/<%= topic._id %>"><%= topic.title %></a>, by <%= topic.author %>.</h3>
      </div>
    <% }) %>
  </body>
</html>
